}

/* Tab Widget styling */
QTabWidget::pane {
    background-color: #645B4E;
    border: 3px solid #3E2B22;
    border-radius: 20px;
    padding: 5px;
}

QTabWidget::tab-bar {
    alignment: center;
}

QTabBar::tab {
    background-color: #F2EEEB;
    color: #645B4E;
    font-weight: bold;
    border: 2px solid #3E2B22;
    border-bottom: none;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    padding: 8px 15px;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background-color: #645B4E;
    color: #D7CBBD;
    border: 2px solid #3E2B22;
    border-bottom: none;
}

QTabBar::tab:hover:!selected {
    background-color: #B7AC9B;
}

/* Styling for content inside tabs */
QWidget#Filters_tab, QWidget#FrequencyFilters_tab, QWidget#Noise_tab, 
QWidget#Edges_tab, QWidget#Equalization_tab, QWidget#Normalization_tab,
QWidget#Thresholding_tab, QWidget#Hybrid_tab, QWidget#Histograms_tab {
    background-color: #645B4E;
    border-radius: 15px;
}

/* For widgets that will be inside the tabs */
