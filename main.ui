QWidget#Thresholding_tab &gt; QWidget,
QWidget#Thresholding_tab &gt; QWidget,
QWidget#Hybrid_tab &gt; QWidget,
QWidget#Histograms_tab &gt; QWidget {
    background-color: #F2EEEB;
    border-radius: 10px;
}

/* Push buttons that will be added to tabs */
QPushButton {
    background-color: #F2EEEB;
    border-radius: 10px;
    color: #645B4E;
    font-size: 13px;
    font-weight: 700;
    border: 3px solid #3E2B22;
    letter-spacing: 0.8px;
    line-height: 20px;
    text-align: center;
    text-transform: uppercase;
    padding: 8px 15px;
}

QPushButton:hover {
    background-color: #B7AC9B;
}

/* Additional styling for common widgets that might be used in tabs */
